if {1} 42 print end # print 42
if {0} 24 print end # don't print 24

# equality
if {0 0 ==} "PASS: 0 == 0" else "ERROR: 0 == 0" end
if {0 1 ==} "ERROR: 0 != 1" else "PASS: 0 != 1" end
if {1 0 ==} "ERROR: 1 != 0" else "PASS: 1 != 0" end
if {1 1 ==} "PASS: 1 == 1" else "ERROR: 1 == 1" end

# greater
if {0 0 >} "ERROR: 0 > 0" else "PASS 0 > 0" end
if {0 1 >} "ERROR: 0 > 1" else "PASS 0 > 1" end
if {1 0 >} "PASS: 1 > 0" else "ERROR 1 > 0" end
if {1 1 >} "ERROR: 0 > 1" else "PASS 0 > 1" end