# equality
0 0 == if "PASS: 0 == 0" else "ERROR: 0 == 0" end
0 1 == if "ERROR: 0 != 1" else "PASS: 0 != 1" end
1 0 == if "ERROR: 1 != 0" else "PASS: 1 != 0" end
1 1 == if "PASS: 1 == 1" else "ERROR: 1 == 1" end

# greater
0 0 > if "ERROR: 0 > 0" else "PASS 0 > 0" end
0 1 > if "ERROR: 0 > 1" else "PASS 0 > 1" end
0 1 > if "ERROR: 0 > 1" else "PASS 0 > 1" end